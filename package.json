{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "dev": "npm-run-all build --parallel \"dev:*\"",
    "dev:remix": "remix watch",
    "dev:wrangler": "cross-env NODE_ENV=development npm run wrangler --binding $(cat .env)",
    "start": "cross-env NODE_ENV=production npm run wrangler",
    "typecheck": "tsc",
    "wrangler": "wrangler pages dev ./public -k PRIVATE_NOTES",
    "lint": "eslint --ext \".js,.jsx,.ts,.tsx\" --ignore-path .gitignore .",
    "prettier": "prettier --write --ignore-path .gitignore .",
    "test:e2e": "playwright test"
  },
  "dependencies": {
    "@floating-ui/react": "^0.22.0",
    "@headlessui/react": "^1.7.13",
    "@heroicons/react": "^2.0.16",
    "@paralleldrive/cuid2": "^2.2.0",
    "@remix-run/cloudflare": "^1.15",
    "@remix-run/cloudflare-pages": "^1.15",
    "@remix-run/react": "^1.15",
    "base64-encoding": "^0.15.0-alpha.0",
    "cross-env": "^7.0.3",
    "prettier": "^2.8.4",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-json-tree": "^0.18.0",
    "react-simple-code-editor": "^0.13.1",
    "use-keyboard-shortcut": "^1.1.6",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.18.0",
    "@playwright/test": "^1.31.2",
    "@remix-run/dev": "^1.15",
    "@remix-run/eslint-config": "^1.15",
    "@remix-run/testing": "^1.15",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@types/prismjs": "^1.26.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/uuid": "^9.0.1",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.36.0",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.5",
    "wrangler": "^2.12.3"
  },
  "engines": {
    "node": ">=16.13"
  }
