{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "utiliti",
  "compatibility_date": "2025-11-17",
  "compatibility_flags": ["nodejs_compat"],
  "workers_dev": true,
  "main": "./build/worker/index.js",
  "assets": {
    "directory": "./build/client",
    "binding": "ASSETS",
    "run_worker_first": true,
  },
  "kv_namespaces": [
    {
      "binding": "PRIVATE_NOTES",
      "id": "PRIVATE_NOTES",
    },
  ],
}
